{
  "name": "http-client-middleware",
  "version": "0.1.0",
  "description": "Http client middleware",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Flexbase",
  "license": "MIT",
  "homepage": "https://github.com/flexbase-eng/http-client-middleware",
  "repository": {
    "type": "git",
    "url": "git@github.com:flexbase-eng/http-client-middleware.git"
  },
  "bugs": {
    "url": "https://github.com/flexbase-eng/http-client-middleware/issues"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/luxon": "^2.3.2",
    "@types/node": "^17.0.41",
    "@types/node-fetch": "^2.6.1",
    "jest": "^28.1.1",
    "jest-extended": "^2.0.0",
    "moq.ts": "^9.0.2",
    "msw": "^0.42.1",
    "node-fetch": "^2.6.7",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "tsconfig.json": "^1.0.11",
    "typedoc": "^0.22.17",
    "typedoc-github-wiki-theme": "^1.0.1",
    "typedoc-plugin-markdown": "^3.12.1",
    "typescript": "^4.7.3",
    "wretch": "^1.7.10"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "docs": "typedoc",
    "prepare": "yarn run build",
    "test": "jest --no-cache"
  },
  "dependencies": {
    "luxon": "^2.4.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tests/tsconfig.tests.json"
      }
    },
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended",
      "<rootDir>/tests/setup.ts"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "moduleDirectories": [
      "<rootDir>/src/",
      "<rootDir>/tests/",
      "node_modules"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  }
}
